{
  "name": "windows-ui-emulation",
  "version": "1.0.0",
  "description": "SPA Emulation of windows 8 UI (React + Redux example) using Atomic Design Arch.",
  "main": "webpack.config.js",
  "scripts": {
    "clean": "rimraf ./dist ./dist-raw ./gen ./assets.json ./scheme-generated.json",
    "start": "npm run dev",
    "dev": "webpack-dev-server --colors",
    "build:babel": "babel --presets es2015 ./src -d dist-raw",
    "build:scheme": "node ./scripts/build-scheme.js",
    "build:webpack": "webpack -p --hide-modules --display-error-details --config ./webpack.config.js",
    "copy:static": "cp -R static/* dist/",
    "build:images": "bash scripts/build-images.sh",
    "build:service": "node scripts/create-service-worker.js",
    "build:static": "node ./dist-raw/generate-static.js",
    "build:qr": "node scripts/build-qr",
    "build:additional": "run-s copy:static build:images build:service build:static build:qr",
    "build": "NODE_ENV=production run-s clean build:babel build:scheme build:webpack build:additional",
    "postversion": "git push && git push --tags",
    "test": "eslint .",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArtemMaximum/task-manager.git"
  },
  "keywords": [
    "Windows",
    "ui",
    "React",
    "Redux",
    "Atomic"
  ],
  "author": "Artem Maximum <artem.habrahabrov@gmail.com> (http://artem.habrahabrov@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArtemMaximum/windows-ui-emulation/issues"
  },
  "homepage": "https://github.com/ArtemMaximum/windows-ui-emulation#readme",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-inline-react-svg": "^0.4.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-styled-name": "^1.0.0",
    "babel-plugin-syntax-export-extensions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.9",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.28.8",
    "eslint": "^4.8.0",
    "eslint-config-atomix-react": "^3.4.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.6.0",
    "qr-image": "^3.2.0",
    "react-hot-loader": "^3.1.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.19.1",
    "svg-inline-loader": "^0.8.0",
    "text-loader": "0.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "except": "^0.1.3",
    "qs": "^6.5.1",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-color": "^2.13.8",
    "react-document-title": "^2.0.2",
    "react-dom": "^16.0.0",
    "react-modal": "^3.1.11",
    "react-modal-dialog": "^4.0.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.2.1",
    "styled-theme": "^0.3.3"
  }
}
