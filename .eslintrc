{
  "parser": "babel-eslint",
  "extends": "atomix-react",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "import/extensions": "off",
    "unicorn/filename-case": "off",
    "react/sort-comp": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "object-curly-newline": "off",
    "object-curly-spacing": "off",
    "function-paren-newline": "off",
    "linebreak-style": "off"
  }
}
